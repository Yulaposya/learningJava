public class Main
{
    static class LinkedList {
        Node head;
        
        class Node {
            int value;
            Node next;
            Node (int num) {
                value = num;
            }
        }
        
        public void insert (int num) {
            Node new_n = new Node (num);
            if (head == null) {
                head = new_n;
            }
            else {
                Node last_n = head;
                while (last_n.next != null) {
                    last_n = last_n.next;
                }
                last_n.next = new_n;
            }
        }
        
        public void show() {
            Node s_node = head;
            while (s_node.next != null) {
                System.out.print (s_node.value + " ");
                s_node = s_node.next;
            }
            System.out.print(s_node.value + "\n");
        }
    }
    
	public static void main(String[] args) {
		LinkedList ll = new LinkedList();
		ll.insert(7);
		ll.insert(7);
		ll.insert(77);
		ll.insert(777);
		ll.show();
	}
}
